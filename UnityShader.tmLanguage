		<dict>
			<key>comment</key>
			<string>Type</string>
			<key>match</key>
			<string>"[^"]+"</string>
			<key>name</key>
			<string>constant.string.source.shader</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Type</string>
			<key>match</key>
			<string>^\s*#(pragma|include).*$</string>
			<key>name</key>
			<string>string.meta.source.shader</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Type</string>
			<key>match</key>
			<string>tex2D|UnpackNormal|saturate|dot|normalize|clip|frac</string>
			<key>name</key>
			<string>constant.source.shader</string>
